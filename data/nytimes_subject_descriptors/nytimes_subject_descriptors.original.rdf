    <skos:scopeNote xml:lang="en">Used for all games played on video game consoles or computer platforms.&lt;br&gt;</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <nyt:first_use rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2004-09-02</nyt:first_use>
